{
  "name": "python-argo-x", 
  "version": "1.0.0",
  "description": "Python版本的Argo隧道代理工具，通过Cloudflare Argo隧道提供Xray订阅链接。",
  "main": "main.py",
  "author": "dogchild",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dogchild/python-argo-x.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "python main.py",
    "install": "pip install -r requirements.txt",
    "dev": "python main.py",
    "test": "python -m pytest tests/ -v"
  },
  "bin": {
    "python-argo-x": "main.py"
  },
  "files": [
    "*.py",
    "requirements.txt",
    "README.md",
    "LICENSE",
    "Dockerfile"
  ],
  "keywords": [
    "python",
    "xray",
    "argo",
    "cloudflare",
    "tunnel",
    "proxy",
    "subscription",
    "v2ray",
    "vmess", 
    "vless",
    "trojan"
  ],
  "engines": {
    "python": ">=3.7"
  },
  "python_requirements": {
    "fastapi": ">=0.104.1",
    "httpx": ">=0.25.2", 
    "python-dotenv": ">=1.0.0",
    "aiofiles": ">=23.2.1",
    "uvicorn": ">=0.24.0"
  }
}